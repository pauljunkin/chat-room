<h2 id = "header">JwC <span class="badge badge-secondary">No Cap</span></h2>
<br>
<ul class="list-group" id = "messages">
	{{#each data}}
		<li class="list-group-item" style="font-size:40px">{{this}}</li>
	{{/each}}
</ul>
<form id="box" action="">
	<div class="form-group row">
		<div class="col-sm-8">
			<label id = "label" style="font-size:30px">Message:</label>
			<textarea class="form-control" rows="3" name = "specialRequests" id="m" autocomplete="off" style="font-size:30px"></textarea>
		</div>
	</div>
	<div class="form-group row">
		<div class="col-sm-2">
			<button type="button" class="btn btn-primary" id = "send" style="font-size:30px">Send</button>
		</div>
	</div>
</form>